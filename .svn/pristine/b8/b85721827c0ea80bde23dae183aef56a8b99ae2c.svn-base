@model HIS.Domain.Models.Module.ModulePermission
@{
    ViewBag.Title = "ModulePermission";
    Layout = null;
}
<section>
    @Styles.Render("~/bundles/FormDesign")
</section>

        <form action="@Url.Action("ModulePermissionForm","Module")" class="form-horizontal" id="frmModulePermission">
            @Html.HiddenFor(a => a.ModulePermissionId)
            @Html.HiddenFor(a => a.OrganizationId)
            @Html.HiddenFor(a => a.CreatedDate)
            @Html.HiddenFor(a => a.CreatedUserId)
            @Html.HiddenFor(a => a.UpdateDate)
            @Html.HiddenFor(a => a.UpdateUserId)

            <div class="form-body" style="margin-top:32px">
                <div class="title"><h1 class="form-section">Menu Permission Detail</h1></div> 
                <div class="row">
                   
                    <div class="col-md-6">
                       
                            <div class="form-group">
                                <label class="control-label col-md-3">Module</label>
                                <div class="col-md-9">
                                    @Html.DropDownListFor(a => a.ModuleId, (List<SelectListItem>)ViewBag.Modules, new { data_fieldname = "Modules", @class = "form-control required",title="Module is required" })

                                    <span class="help-block"></span>
                                </div>
                            </div>
                      
                        <div class="form-group">
                            <label class="control-label col-md-3">Code</label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(a => a.ModulePermissionCode, new { data_fieldname = "ModulePermissionCode", @class = "form-control required", title = "Module permission code is required" })

                                <span class="help-block"></span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-3">Description</label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(a => a.Desription, new { data_fieldname = "ModulePermissionDescription", @class = "form-control required", title = "Module permission description is required" })

                                <span class="help-block"></span>
                            </div>
                        </div>
                    </div>
                    <!--/span-->
                    <!--/span-->
                </div>
                <!--/row-->

            </div>
            <div class="form-actions">
                <div class="row">
                    <div class="col-md-6">
                        <div class="row">
                            <div class="col-md-offset-3 col-md-9">
                                <button type="button" class="btn green" onclick="Global.SubmitFormViaAjax('frmModulePermission')">Submit</button>
                                <button type="reset" class="btn default">Cancel</button>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6"> </div>
                </div>
            </div>
        </form>




